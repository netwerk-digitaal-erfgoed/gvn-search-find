{
  "name": "editorial-gvn",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build-ui": "run-p type-check build-only",
    "build": "npm run build-ui",
    "preview": "vite preview --port 4173",
    "test:sdk": "NODE_OPTIONS=--experimental-vm-modules jest ${1}",
    "test:sdk:watch": "npm test -- --watchAll",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "test:unit": "cypress open --component",
    "test:unit:ci": "cypress run --component --quiet --reporter spec",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "gaxios": "5.x.x",
    "pinia": "^2.0.22",
    "rdf-dereference": "^2.0.0",
    "vue": "^3.2.37",
    "vue-progressive-image": "^4.0.0",
    "vue-router": "^4.1.2",
    "vue3-simple-typeahead": "^1.0.11"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@jest/globals": "29.x.x",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "29.x.x",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.3.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "jest": "29.x.x",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "29.x.x",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vite-plugin-node-polyfills": "^0.5.0",
    "vue-tsc": "^1.0.9"
  }
}
